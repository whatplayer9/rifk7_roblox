        key2 = txt 
    -- this will make key2 whatever you put inside the textbox
    
    end)
    
    Section:NewButton("check key", "checks your key", function() 
    -- this button will check key
    
        if key2 == key then
   loadstring(game:HttpGet('https://pastebin.com/raw/7FNwAxZx'))() 
    makefolder("otsosi_rifku")
        else

            print('fake key')
        end
    end)
    
    
    
